void main(){
    char virgul ;
    virgul = ',';
    int i;
    int n;
    int t1 = 0; 
    int t2 = 1;
    int nextTerm;
    n=20;
    nextTerm = t1 + t2;
    while(i <= n)
    {   
        i++;
        cout << nextTerm;
        cout << virgul;
        t1 = t2;
        t2 = nextTerm;
        nextTerm = t1 + t2;
    }
    cout<<nextTerm;
}